{
  "name": "ts-guardian",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node-fetch": "^1.6.8",
    "jest": "^22.4.3",
    "node-fetch": "^2.6.7",
    "prettier": "^1.11.1",
    "ts-jest": "^22.4.2",
    "typescript": "^2.8.1"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  }
}
